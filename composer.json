{
    "name": "morscate/cloudwatch-logger",
    "description": "Log events to AWS CloudWatch including embedded metrics",
    "keywords": ["AWS", "CloudWatch", "log", "embedded metrics", "metrics"],
    "type": "library",
    "require": {
        "php": "^8.0",
        "aws/aws-sdk-php": "^3.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "Morscate\\CloudwatchLogger\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Rik Morskate",
            "email": "rik@morscate.nl"
        }
    ],
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "Morscate\\CloudwatchLogger\\CloudwatchLoggerServiceProvider"
            ]
        }
    }
}
